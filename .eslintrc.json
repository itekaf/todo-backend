{
    "extends": "airbnb-base",
    "env" : {
        "browser": true,
        "node": true,
        "es6" : true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "import/extensions": "browser",
        "indent": ["error", 4],
        "no-console": "warn"
    }
}
